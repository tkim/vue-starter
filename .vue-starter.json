{
  "generators": {
    "blueprintDirectory": "./tools/generators",
    "outputDirectory": "./src/app"
  },
  "i18n": {
    "defaultLocale": "en",
    "supportedLocales": ["en", "de", "pt", "zh-cn"],
    "outputDirectory": "./i18n"
  },
  "jest": {
    "roots": ["./src"],
    "verbose": true,
    "globals": {
      "window": true
    },
    "setupFiles": ["./jestsetup.ts"],
    "moduleFileExtensions": ["js", "vue", "ts", "json"],
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "coveragePathIgnorePatterns": ["/node_modules/", "<rootDir>/jestsetup.ts"],
    "coverageDirectory": "./coverage"
  }
}
