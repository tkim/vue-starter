{
  "generators": {
    "blueprintDirectory": "./tools/generators",
    "outputDirectory": "./src/app"
  },
  "i18n": {
    "defaultLocale": "en",
    "supportedLocales": ["en", "de", "pt", "zh-cn"]
  },
  "jest": {
    "globals": {
      "window": true
    },
    "rootDir": ".",
    "roots": ["<rootDir>/src"],
    "setupFiles": ["<rootDir>/jestsetup.ts"],
    "moduleFileExtensions": ["js", "vue", "ts", "json"],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "coveragePathIgnorePatterns": ["/node_modules/", "<rootDir>/jestsetup.ts"],
    "coverageDirectory": "./coverage"
  }
}
