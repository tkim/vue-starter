{
  "name": "vue-starter",
  "version": "1.4.0",
  "description": "A flexible, scalable, opinionated boilerplate for production-ready PWAs with focus on performance, development speed, and best practices",
  "repository": {
    "type": "git",
    "url": "https://github.com/devCrossNet/vue-starter.git"
  },
  "keywords": [
    "vue",
    "vuex",
    "vue-router",
    "webpack",
    "typescript",
    "server-side-rendering",
    "boilerplate",
    "jest",
    "enterprise",
    "datepicker",
    "slider",
    "forms",
    "components",
    "design-system"
  ],
  "author": "Johannes Werner <johannes.werner86@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/devCrossNet/vue-starter",
  "bugs": {
    "url": "https://github.com/devCrossNet/vue-starter/issues"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test vue-starter-service test",
    "extract-i18n-messages": "vue-starter-service em",
    "generate": "vue-starter-service g",
    "lint": "vue-starter-service lint",
    "e2e": "vue-starter-service e2e",
    "clean": "vue-starter-service clean",
    "storybook:dev": "vue-starter-service storybook --dev",
    "storybook:build": "vue-starter-service storybook",
    "prettier": "npm run clean && ./node_modules/.bin/prettier --write \"./**/*.{ts,vue,js,json,scss,sass,css,yaml,yml}\"",
    "release:major": "changelog -M && git add CHANGELOG.md && git commit -m 'chore: update changelog' && npm version major && git push origin && git push origin --tags",
    "release:minor": "changelog -m && git add CHANGELOG.md && git commit -m 'chore: update changelog' && npm version minor && git push origin && git push origin --tags",
    "release:patch": "changelog -p && git add CHANGELOG.md && git commit -m 'chore: update changelog' && npm version patch && git push origin && git push origin --tags",
    "dev": "npm run dev:dev-server && npm run dev:server",
    "build": "concurrently \"npm run build:client\" \"npm run build:isomorphic\" \"npm run build:server\"",
    "analyze-bundle": "cross-env ANALYZE=true npm run build:client",
    "update": "npm run compile-tools && node ./tools/scripts/update.js",
    "compile-tools": "./node_modules/.bin/tsc -p ./tools/",
    "start": "node ./dist/server/server.js",
    "dev:dev-server": "cross-env NODE_ENV=development webpack --mode development --config ./tools/webpack/dev-server.js",
    "dev:server": "cross-env NODE_ENV=development webpack --mode development --config ./tools/webpack/server.js",
    "predev": "npm run clean && npm run compile-tools",
    "build:spa": "npm run clean && cross-env NODE_ENV=production webpack --mode production --config ./tools/webpack/spa.js",
    "build:client": "cross-env NODE_ENV=production webpack --mode production --config ./tools/webpack/client.js",
    "build:isomorphic": "cross-env NODE_ENV=production webpack --mode production --config ./tools/webpack/isomorphic.js",
    "build:server": "cross-env NODE_ENV=production webpack --mode production --config ./tools/webpack/server.js",
    "prebuild": "npm run clean && npm run compile-tools",
    "ci": "concurrently \"npm run lint\" \"npm run test\"",
    "postci": "codecov -f ./coverage/*.json && npm run build && concurrently \"npm run storybook:build\" \"vuepress build docs\"",
    "heroku-postbuild": "npm run build",
    "postinstall": "npm run compile-tools"
  },
  "dependencies": {
    "accept-language": "3.0.18",
    "animejs": "2.2.0",
    "axios": "0.18.0",
    "body-parser": "1.18.3",
    "compression": "1.7.3",
    "config": "3.0.1",
    "cookie-parser": "1.4.3",
    "deepmerge": "3.0.0",
    "express": "4.16.4",
    "js-cookie": "2.2.0",
    "lodash": "4.17.11",
    "lru-cache": "5.1.1",
    "marked": "0.5.2",
    "serve-favicon": "2.5.0",
    "vee-validate": "2.0.9",
    "vue": "2.5.21",
    "vue-i18n": "8.6.0",
    "vue-meta": "1.5.8",
    "vue-router": "3.0.2",
    "vue-server-renderer": "2.5.21",
    "vuex": "3.0.1",
    "vuex-router-sync": "5.0.0",
    "winston": "3.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "7.2.1",
    "@commitlint/config-conventional": "7.1.2",
    "@types/animejs": "2.0.1",
    "@types/body-parser": "1.17.0",
    "@types/compression": "0.0.36",
    "@types/cookie-parser": "1.4.1",
    "@types/express": "4.16.0",
    "@types/jest": "23.3.10",
    "@types/js-cookie": "2.2.0",
    "@types/lodash": "4.14.119",
    "@types/marked": "0.5.0",
    "@types/node": "10.12.18",
    "@types/serve-favicon": "2.2.30",
    "@types/storybook__addon-a11y": "3.3.1",
    "@types/storybook__addon-actions": "3.4.1",
    "@types/storybook__addon-options": "3.2.2",
    "@types/storybook__vue": "3.3.1",
    "@types/webpack": "4.4.22",
    "@types/webpack-dev-middleware": "2.0.2",
    "@types/webpack-merge": "4.1.3",
    "@vue/test-utils": "1.0.0-beta.27",
    "autoprefixer": "9.4.3",
    "axios-mock-adapter": "1.15.0",
    "codecov": "3.1.0",
    "compression-webpack-plugin": "2.0.0",
    "concurrently": "4.1.0",
    "copy-webpack-plugin": "4.6.0",
    "cross-env": "5.2.0",
    "css-loader": "2.1.0",
    "css-mqpacker": "7.0.0",
    "cssnano": "4.1.8",
    "file-loader": "2.0.0",
    "fork-ts-checker-webpack-plugin": "0.5.2",
    "generate-changelog": "1.7.1",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.3.1",
    "identity-obj-proxy": "3.0.0",
    "node-sass": "4.11.0",
    "postcss-loader": "3.0.0",
    "prettier": "1.15.3",
    "pretty-quick": "1.8.0",
    "sass-loader": "7.1.0",
    "serviceworker-webpack-plugin": "1.0.1",
    "start-server-webpack-plugin": "2.2.5",
    "ts-jest": "22.4.6",
    "ts-loader": "5.3.2",
    "typescript": "3.2.2",
    "vue-loader": "15.4.2",
    "vue-ssr-webpack-plugin": "3.0.0",
    "vue-template-compiler": "2.5.21",
    "webpack": "4.28.2",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-cli": "3.1.2",
    "webpack-dev-middleware": "3.4.0",
    "webpack-hot-middleware": "2.24.3",
    "webpack-merge": "4.1.5",
    "webpack-node-externals": "1.7.2"
  },
  "engines": {
    "node": "10.14.1",
    "npm": "6.4.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged && concurrently \"npm run lint\" \"npm run test\""
    }
  }
}
